#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nitishanand/Desktop/shipit-v2/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.6_@babel+core@7.24.6__@babel+plugin-transf_chg5mb7pdciqbmzpdizft7a7ye/node_modules/react-scripts/bin/node_modules:/Users/nitishanand/Desktop/shipit-v2/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.6_@babel+core@7.24.6__@babel+plugin-transf_chg5mb7pdciqbmzpdizft7a7ye/node_modules/react-scripts/node_modules:/Users/nitishanand/Desktop/shipit-v2/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.6_@babel+core@7.24.6__@babel+plugin-transf_chg5mb7pdciqbmzpdizft7a7ye/node_modules:/Users/nitishanand/Desktop/shipit-v2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nitishanand/Desktop/shipit-v2/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.6_@babel+core@7.24.6__@babel+plugin-transf_chg5mb7pdciqbmzpdizft7a7ye/node_modules/react-scripts/bin/node_modules:/Users/nitishanand/Desktop/shipit-v2/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.6_@babel+core@7.24.6__@babel+plugin-transf_chg5mb7pdciqbmzpdizft7a7ye/node_modules/react-scripts/node_modules:/Users/nitishanand/Desktop/shipit-v2/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.6_@babel+core@7.24.6__@babel+plugin-transf_chg5mb7pdciqbmzpdizft7a7ye/node_modules:/Users/nitishanand/Desktop/shipit-v2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
fi
